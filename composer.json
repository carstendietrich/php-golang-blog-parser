{
    "name": "carstendietrich/php-golang-blog-parser",
    "description": "Parse blog.golang.org with PHP and expose via JSON API",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "ext-bcmath": "*",
        "ext-gmp": "*",
        "ext-json": "*",
        "laravel/lumen-framework": "^8.0",
        "opencensus/opencensus": "~0.2",
        "opencensus/opencensus-exporter-jaeger": "~0.1",
        "paquettg/php-html-parser": "^3.1",
        "ralouphie/getallheaders": "^3.0",
        "ramsey/uuid": "4 as 3"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
